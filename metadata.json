{
  "name": "muppet-unifi",
  "version": "0.1.0",
  "author": "Scotty Logan <scotty@catbert.net>",
  "summary": "Configuration for a Unifi Controller muppet",
  "license": "MIT",
  "source": "https://github.com/scottylogan/muppet-unifi.git",
  "project_page": "https://github.com/scottylogan/muppet-unifi",
  "issues_url": "https://github.com/scottylogan/muppet-unifi/issues",

  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemmajrelease": [ "8", "9" ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemmajrelease": [ "16.04" ]
    }
  ],

  "requirements": [
    { "name": "puppet", "version_requirement": "4.x" }
  ],

  "dependencies": [
    { "name": "puppetlabs-stdlib",    "version_requirement": ">= 1.0.0" },
    { "name": "puppetlabs-apt",       "version_requirement": ">= 2.2.0" }
  ]
}
